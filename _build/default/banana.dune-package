(lang dune 2.9)
(name banana)
(version dev)
(library
 (name banana.basics)
 (kind normal)
 (synopsis "Basics Imprementations de graphes de flot et fonctionnelles")
 (archives (byte basics/basics.cma) (native basics/basics.cmxa))
 (plugins (byte basics/basics.cma) (native basics/basics.cmxs))
 (native_archives basics/basics.a)
 (requires ocamlgraph)
 (main_module_name Basics)
 (modes byte native)
 (modules
  (wrapped
   (main_module_name Basics)
   (modules
    ((name Dynamic_array)
     (obj_name basics__Dynamic_array)
     (visibility public)
     (impl)
     (intf))
    ((name Flow_graph)
     (obj_name basics__Flow_graph)
     (visibility public)
     (impl)
     (intf))
    ((name Linked_array)
     (obj_name basics__Linked_array)
     (visibility public)
     (impl)
     (intf)))
   (alias_module
    (name Basics)
    (obj_name basics)
    (visibility public)
    (kind alias)
    (impl))
   (wrapped true))))
